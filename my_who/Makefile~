CC = gcc
CFLAGS = -Wall -g 
# flags -Wall warning all, -g debug
OBJETOS = quien1.o quien2.o quien3.o 

PROGS = quien1 quien2 quien3

all: $(PROGS)

quien1: quien1.o 
	$(CC) -g -o $@ $<

quien2: quien2.o 
	$(CC) -g -o $@ $<

quien3: quien3.o 
	$(CC) -g -o $@ $<

#$(OBJETOS): quien.h     NO USAMOS quien.h

%.o : %.c 
	gcc -c $(CFLAGS) $<

clean :
	-rm $(OBJETOS) $(PROGS) 	
